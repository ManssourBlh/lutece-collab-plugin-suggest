<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

    <!-- Plugin Informations -->
    <name>digglike</name>
    <class>fr.paris.lutece.plugins.digglike.service.DigglikePlugin</class>
    <version>2.2.12</version>
    <description>digglike.plugin.description</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>digglike.plugin.description</description>
    <provider>digglike.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/digglike/digglike.png</icon-url>
    <copyright>Copyright 2001-2011 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>2.4.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>
    <!-- Specific plugin CSS stylesheet -->
    <css-stylesheets>
        <css-stylesheets-scope>xpage</css-stylesheets-scope>
        <css-stylesheet>../../js/jquery/plugins/star-rating/jquery.rating.css</css-stylesheet>
       	<css-stylesheet>digglike/digglike.css</css-stylesheet>
    </css-stylesheets>
    <!-- Specific plugin JAVASCRIPT -->
    <javascript-files>
        <javascript-files-scope>xpage</javascript-files-scope>
        <javascript-file>digglike/digglike.js</javascript-file>
        <javascript-file>../jquery/plugins/star-rating/jquery.rating.js</javascript-file>
    </javascript-files>

    <!--Administration interface parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>DIGGLIKE_MANAGEMENT</feature-id>
            <feature-title>digglike.adminFeature.digglike_management.name</feature-title>
            <feature-description>digglike.adminFeature.digglike_management.description</feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/digglike/ManageDigg.jsp</feature-url>
            <feature-icon-url>images/admin/skin/plugins/digglike/digg.png</feature-icon-url>
        </admin-feature>
    </admin-features>
    <!-- RBAC Resources -->
    <rbac-resource-types>
        <rbac-resource-type>
            <rbac-resource-type-class>fr.paris.lutece.plugins.digglike.service.DigglikeResourceIdService</rbac-resource-type-class>
        </rbac-resource-type>
        <rbac-resource-type>
            <rbac-resource-type-class>fr.paris.lutece.plugins.digglike.service.DefaultMessageResourceIdService</rbac-resource-type-class>
        </rbac-resource-type>
        <rbac-resource-type>
            <rbac-resource-type-class>fr.paris.lutece.plugins.digglike.service.CategoryResourceIdService</rbac-resource-type-class>
        </rbac-resource-type>
        <rbac-resource-type>
            <rbac-resource-type-class>fr.paris.lutece.plugins.digglike.service.ExportFormatResourceIdService</rbac-resource-type-class>
        </rbac-resource-type>
    </rbac-resource-types>

    <!-- Xpage configuration -->
    <applications>
        <application>
            <application-id>digg</application-id>
            <application-class>fr.paris.lutece.plugins.digglike.web.DiggApp</application-class>
        </application>
    </applications>
    <!-- Search Indexer Service -->
    <search-indexers>
        <search-indexer>
            <search-indexer-class>fr.paris.lutece.plugins.digglike.service.search.DigglikeIndexer</search-indexer-class>
        </search-indexer>
    </search-indexers>

    <daemons>
        <daemon>
            <daemon-id>digglikePublication</daemon-id>
            <daemon-name>digglike.daemon.digglikePublication.name</daemon-name>
            <daemon-description>digglike.daemon.digglikePublication.description</daemon-description>
            <daemon-class>fr.paris.lutece.plugins.digglike.service.publication.DaemonPublication</daemon-class>
        </daemon>
        <daemon>
            <daemon-id>diggSubscribersNotificationDaemon</daemon-id>
            <daemon-name>digglike.daemon.diggSubscribersNotificationDaemon.name</daemon-name>
            <daemon-description>digglike.daemon.diggSubscribersNotificationDaemon.description</daemon-description>
            <daemon-class>fr.paris.lutece.plugins.digglike.service.subscription.DiggSubscribersNotificationDaemon</daemon-class>
        </daemon>
    </daemons>
</plug-in>